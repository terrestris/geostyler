    "tslint": "^5.16.0",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.1",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "whatwg-fetch": "^3.5.0"
  },
  "peerDependencies": {
    "antd": "3.x",
    "ol": "6.x",
    "react": "16.x",
    "react-dom": "16.x"
  }
}
